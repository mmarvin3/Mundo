<body>
    <a class="title-header" href="/countries">
        <h1>Mundo</h1>
    </a>
    <main class="show-main">
        <h2>New Country</h2>
        <form action="/countries" method="POST">
            Name: <input type="text" name="name"></br>
            Flag Image: <input type="text" name="img"></br>
            Capital: <input type="text" name="capital"></br>
            Population: <input type="text" name="population"><br />
            <input type="submit" name="" value="Add Country">

        </form>
    </main>
